class calc{
    int a;
    int b;

    calc(int z){
        this.a = z;
        this.b = z;
    }

    int sum(){
        return this.a + this.b;
    }
}

class dev extends calc{
    dev(int s, int q){
        this.a = s;
        this.b = q;
    }

}

{
